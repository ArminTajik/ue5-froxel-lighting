#include "/Engine/Private/Common.ush"
RWStructuredBuffer<uint> FroxelGrid;

uint GridX;
uint GridY;
uint GridZ;
uint GridCount;

[numthreads(8,8,8)]
void MainCS(uint3 DTid : SV_DispatchThreadID) {
    uint idx = DTid.x + DTid.y * GridX + DTid.z * GridX * GridY;
    if (idx >= GridCount)
        return;
    FroxelGrid[idx] = idx;
}